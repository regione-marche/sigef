          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="ID_BANDO">
          <DataField>ID_BANDO</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Obbligatorio">
