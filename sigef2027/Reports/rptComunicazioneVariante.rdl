    <DataSet Name="dsVariante">
      <Query>
        <DataSourceName>SIGEF_svil</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdVariante">
            <Value>=Parameters!IdVariante.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        IMPRESA.ID_IMPRESA, IMPRESA_AGGREGAZIONE.ID AS ID_IMPRESA_AGGREGAZIONE, TIPO_AGGREGAZIONE_RUOLO.DESCRIZIONE AS DESCRIZIONE_TIPO_AGGREGAZIONE, VARIANTI.ID_VARIANTE, 
                         VARIANTI.ID_PROGETTO, VARIANTI.COD_TIPO, SUBSTRING(VARIANTI.segnatura, CHARINDEX('|', VARIANTI.segnatura)+ 1, 10) as [DATA_INSERIMENTO], VARIANTI.COD_ENTE, VARIANTI.OPERATORE, VARIANTI.DATA_MODIFICA, VARIANTI.DESCRIZIONE, VARIANTI.SEGNATURA, 
                         VARIANTI.SEGNATURA_ALLEGATI, VARIANTI.SEGNATURA_APPROVAZIONE, VARIANTI.APPROVATA, VARIANTI.DATA_APPROVAZIONE, VARIANTI.ISTRUTTORE, VARIANTI.NOTE_ISTRUTTORE, 
                         VARIANTI.CUAA_ENTRANTE, VARIANTI.ID_FASCICOLO_ENTRANTE, VARIANTI.CUAA_USCENTE, VARIANTI.RAGSOC_USCENTE, VARIANTI.ID_FASCICOLO_USCENTE, VARIANTI.ANNULLATA, 
                         VARIANTI.SEGNATURA_ANNULLAMENTO, VARIANTI.CF_OPERATORE_ANNULLAMENTO, VARIANTI.DATA_ANNULLAMENTO, VARIANTI.ID_ATTO_APPROVAZIONE
FROM            PROGETTO INNER JOIN
                         IMPRESA ON IMPRESA.ID_IMPRESA = PROGETTO.ID_IMPRESA INNER JOIN
                         VARIANTI ON PROGETTO.ID_PROGETTO = VARIANTI.ID_PROGETTO LEFT OUTER JOIN
                         IMPRESA_AGGREGAZIONE ON IMPRESA.ID_IMPRESA = IMPRESA_AGGREGAZIONE.ID_IMPRESA LEFT OUTER JOIN
                         TIPO_AGGREGAZIONE_RUOLO ON IMPRESA_AGGREGAZIONE.COD_RUOLO = TIPO_AGGREGAZIONE_RUOLO.CODICE
WHERE        (VARIANTI.ID_VARIANTE = @IdVariante)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
