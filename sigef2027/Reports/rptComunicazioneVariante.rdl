    <DataSet Name="dsChecklist">
      <Query>
        <DataSourceName>SIGEF_svil</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdVariante">
            <Value>=Parameters!IdVariante.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     vVARIANTI_ESITI_REQUISITI.NOTE, vVARIANTI_ESITI_REQUISITI.DESCRIZIONE, vVARIANTI_ESITI_REQUISITI.ESITO_ISTRUTTORE AS EsitoVerifica, 
                      vVARIANTI_ESITI_REQUISITI.ESITO_POSITIVO_ISTRUTTORE, BANDO_REQUISITI_VARIANTE.ID_BANDO, 
                      CASE OBBLIGATORIO WHEN 0 THEN 'NO' ELSE 'SI' END AS Obbligatorio, BANDO_REQUISITI_VARIANTE.ORDINE,
                          (SELECT     CASE WHEN (ESITO_POSITIVO_ISTRUTTORE = 0 OR
                                                   ESITO_POSITIVO_ISTRUTTORE IS NULL) AND OBBLIGATORIO = 1 THEN 1 ELSE 0 END AS Expr1) AS STATO_PROGETTO, 
                      BANDO_REQUISITI_VARIANTE.REQUISITO_DI_ISTRUTTORIA, vUTENTI.NOMINATIVO
FROM         vVARIANTI_ESITI_REQUISITI LEFT OUTER JOIN
                      vUTENTI ON vVARIANTI_ESITI_REQUISITI.ISTRUTTORE = vUTENTI.CF_UTENTE LEFT OUTER JOIN
                      BANDO_REQUISITI_VARIANTE ON vVARIANTI_ESITI_REQUISITI.ID_REQUISITO = BANDO_REQUISITI_VARIANTE.ID_REQUISITO AND 
                      BANDO_REQUISITI_VARIANTE.ID_BANDO =
                          (SELECT     ID_BANDO
                            FROM          PROGETTO
                            WHERE      (ID_PROGETTO =
                                                       (SELECT     ID_PROGETTO
                                                         FROM          VARIANTI
                                                         WHERE      (ID_VARIANTE = @IdVariante)))) AND BANDO_REQUISITI_VARIANTE.COD_TIPO =
                          (SELECT     COD_TIPO
                            FROM          VARIANTI
                            WHERE      (ID_VARIANTE = @IdVariante))
WHERE     (vVARIANTI_ESITI_REQUISITI.ID_VARIANTE = @IdVariante) AND (BANDO_REQUISITI_VARIANTE.REQUISITO_DI_ISTRUTTORIA = 1) 
ORDER BY BANDO_REQUISITI_VARIANTE.ORDINE</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="NOTE">
          <DataField>NOTE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DESCRIZIONE">
          <DataField>DESCRIZIONE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EsitoVerifica">
