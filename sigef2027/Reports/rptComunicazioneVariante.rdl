  <DataSets>
    <DataSet Name="DatiGenerali">
      <Query>
        <DataSourceName>SIGEF_svil</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdVariante">
            <Value>=Parameters!IdVariante.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        IMPRESA.CUAA, IMPRESA_STORICO.RAGIONE_SOCIALE, vINDIRIZZARIO.VIA, vINDIRIZZARIO.COMUNE, vINDIRIZZARIO.CAP, vINDIRIZZARIO.SIGLA, PERSONA_FISICA.NOME, PERSONA_FISICA.COGNOME, 
                         vINDIRIZZARIO.PEC, IMPRESA.CODICE_FISCALE
FROM            IMPRESA INNER JOIN
                         PROGETTO ON IMPRESA.ID_IMPRESA = PROGETTO.ID_IMPRESA INNER JOIN
                         IMPRESA_STORICO ON IMPRESA.ID_STORICO_ULTIMO = IMPRESA_STORICO.ID_STORICO_IMPRESA INNER JOIN
                         vINDIRIZZARIO ON IMPRESA.ID_SEDELEGALE_ULTIMO = vINDIRIZZARIO.ID_INDIRIZZARIO INNER JOIN
                         PERSONE_X_IMPRESE ON IMPRESA.ID_RAPPRLEGALE_ULTIMO = PERSONE_X_IMPRESE.ID_PERSONE_X_IMPRESE INNER JOIN
                         PERSONA_FISICA ON PERSONE_X_IMPRESE.ID_PERSONA = PERSONA_FISICA.ID_PERSONA INNER JOIN
                         VARIANTI ON PROGETTO.ID_PROGETTO = VARIANTI.ID_PROGETTO
WHERE        (VARIANTI.ID_VARIANTE = @IdVariante)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
