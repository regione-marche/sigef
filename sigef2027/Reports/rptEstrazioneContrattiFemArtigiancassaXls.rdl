    <DataSet Name="DsTotali">
      <Query>
        <DataSourceName>SIGEF_TEST</DataSourceName>
        <CommandText>
select 
	(select 
		SUM(ISNULL(cf.IMPORTO , 0.00)) as IMPORTO_EROGATO
	from
		vPROGETTO p 
		inner join vBANDO_CONFIG bc on bc.ID_BANDO = p.ID_BANDO and bc.ATTIVO = 1 and bc.COD_TIPO = 'UTSTRUMFIN' 
		inner join BANDO_PROGRAMMAZIONE bp on bp.ID_BANDO = bc.ID_BANDO
		inner join vIMPRESA imp on imp.ID_IMPRESA = p.ID_IMPRESA
		inner join VCONTRATTI_FEM cf on cf.ID_PROGETTO = p.ID_PROGETTO 
	where 
		p.ORDINE_FASE &gt; 3 and 
		ORDINE_STATO = 1
	)
	+
	(select 
		SUM(ISNULL(GIU.IMPONIBILE, 0.00)) as IMPORTO_CONTRATTO
	from
		vPROGETTO p 
		inner join BANDO_PROGRAMMAZIONE bp on bp.ID_BANDO = p.ID_BANDO
		inner join vIMPRESA imp on imp.ID_IMPRESA = p.ID_IMPRESA
		inner join VCONTRATTI_FEM cf on cf.ID_PROGETTO = p.ID_PROGETTO 
		inner JOIN GIUSTIFICATIVI GIU ON CF.ID_GIUSTIFICATIVO = GIU.ID_GIUSTIFICATIVO
	where 
		p.ORDINE_FASE &gt; 3  
		and ORDINE_STATO = 1 
		and cf.ID_GIUSTIFICATIVO  is not null 
	)	
		 as TOT_IMPORTO_CONTRATTI,
		
	(select 
		SUM(ISNULL(cfp.IMPORTO , 0.00)) as IMPORTO_EROGATO
	from
		vPROGETTO p 
		inner join vBANDO_CONFIG bc on bc.ID_BANDO = p.ID_BANDO and bc.ATTIVO = 1 and bc.COD_TIPO = 'UTSTRUMFIN' 
		inner join BANDO_PROGRAMMAZIONE bp on bp.ID_BANDO = bc.ID_BANDO
		inner join vIMPRESA imp on imp.ID_IMPRESA = p.ID_IMPRESA
		
		inner join VCONTRATTI_FEM cf on cf.ID_PROGETTO = p.ID_PROGETTO 
		left outer join VCONTRATTI_FEM_PAGAMENTI cfp on cfp.ID_CONTRATTO_FEM = cf.ID_CONTRATTO_FEM
	where 
		p.ORDINE_FASE &gt; 3 and 
		ORDINE_STATO = 1
	)
	+
	(select 
		SUM(ISNULL(cfp.IMPORTO , 0.00)) as IMPORTO_EROGATO
	from
		vPROGETTO p 
		inner join BANDO_PROGRAMMAZIONE bp on bp.ID_BANDO = p.ID_BANDO
		inner join vIMPRESA imp on imp.ID_IMPRESA = p.ID_IMPRESA
		inner join VCONTRATTI_FEM cf on cf.ID_PROGETTO = p.ID_PROGETTO 
		left outer join VCONTRATTI_FEM_PAGAMENTI cfp on cfp.ID_CONTRATTO_FEM = cf.ID_CONTRATTO_FEM
		inner JOIN GIUSTIFICATIVI GIU ON CF.ID_GIUSTIFICATIVO = GIU.ID_GIUSTIFICATIVO
	where 
		p.ORDINE_FASE &gt; 3  
		and ORDINE_STATO = 1 
		and cf.ID_GIUSTIFICATIVO  is not null
	) as TOT_IMPORTO_EROGATO

	</CommandText>
      </Query>
      <Fields>
        <Field Name="TOT_IMPORTO_CONTRATTI">
          <DataField>TOT_IMPORTO_CONTRATTI</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TOT_IMPORTO_EROGATO">
          <DataField>TOT_IMPORTO_EROGATO</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
