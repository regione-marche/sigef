    <DataSource Name="SIGEF_TEST">
      <DataSourceReference>SIGEF</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>f26f1cc2-a63e-443d-8b0b-cf3985f80f9c</rd:DataSourceID>
