  <DataSets>
    <DataSet Name="DsContratti">
      <Query>
        <DataSourceName>SIGEF_TEST</DataSourceName>
        <CommandText>
WITH INTERVENTI AS
(
SELECT  ASSE.ID ID_ASSE,
        ASSE.CODICE COD_ASSE,
        ASSE.DESCRIZIONE DESC_ASSE,
        AZIONE.ID ID_AZIONE,
        AZIONE.CODICE COD_AZIONE,
        AZIONE.DESCRIZIONE DESC_AZIONE,
        IP.ID ID_INTERVENTO,
        IP.CODICE COD_INTERVENTO,
        IP.DESCRIZIONE DESC_INTERVENTO
FROM    zPROGRAMMAZIONE IP
        INNER JOIN 
        zPROGRAMMAZIONE AZIONE ON AZIONE.ID = IP.ID_PADRE
        INNER JOIN 
        zPROGRAMMAZIONE ASSE ON ASSE.ID = AZIONE.ID_PADRE
WHERE 1 = 1
  AND IP.COD_TIPO = 'POR20142020L3'
  AND IP.ID NOT IN (17,18,19)
--ORDER BY ASSE.CODICE, AZIONE.CODICE, IP.CODICE
)


select 
	'ASSE '+i.COD_ASSE+' - AZIONE '+i.COD_AZIONE+' - Intervento '+i.COD_INTERVENTO+' '+i.DESC_INTERVENTO as INTERVENTO,
                   i.ID_INTERVENTO,
	imp.RAGIONE_SOCIALE,
	imp.CODICE_FISCALE,
	cf.ID_CONTRATTO_FEM,
	ISNULL(CF.IMPORTO,  0.00) as IMPORTO_CONTRATTO,
	CF.DATA_STIPULA_CONTRATTO as DATA_STIPULA_CONTRATTO,
	ISNULL(cfp.IMPORTO , 0.00) as IMPORTO_EROGATO,
	cfp.DATA as DATA_PAGAMENTO,STR( p.ID_PROGETTO) as PROG
from
	vPROGETTO p 
	inner join vBANDO_CONFIG bc on bc.ID_BANDO = p.ID_BANDO and bc.ATTIVO = 1 and bc.COD_TIPO = 'UTSTRUMFIN' 
	inner join BANDO_PROGRAMMAZIONE bp on bp.ID_BANDO = bc.ID_BANDO
	inner join INTERVENTI i on i.ID_INTERVENTO = bp.ID_PROGRAMMAZIONE
	inner join vIMPRESA imp on imp.ID_IMPRESA = p.ID_IMPRESA
	
	inner join VCONTRATTI_FEM cf on cf.ID_PROGETTO = p.ID_PROGETTO 
	left outer join VCONTRATTI_FEM_PAGAMENTI cfp on cfp.ID_CONTRATTO_FEM = cf.ID_CONTRATTO_FEM
	


where 
	p.ORDINE_FASE &gt; 3 and 
	ORDINE_STATO = 1 

	

                   
UNION 

select 
	'GIUSTIFICATIVO' as INTERVENTO,
                   i.ID_INTERVENTO,
	imp.RAGIONE_SOCIALE,
	imp.CODICE_FISCALE,
	cf.ID_CONTRATTO_FEM,
	ISNULL(GIU.IMPONIBILE, 0.00) as IMPORTO_CONTRATTO,
	GIU.DATA as DATA_STIPULA_CONTRATTO,
	ISNULL(cfp.IMPORTO , 0.00) as IMPORTO_EROGATO,
	cfp.DATA as DATA_PAGAMENTO,'' as PROG
from
	vPROGETTO p 
	
	inner join BANDO_PROGRAMMAZIONE bp on bp.ID_BANDO = p.ID_BANDO
	inner join INTERVENTI i on i.ID_INTERVENTO = bp.ID_PROGRAMMAZIONE
	inner join vIMPRESA imp on imp.ID_IMPRESA = p.ID_IMPRESA
	
	inner join VCONTRATTI_FEM cf on cf.ID_PROGETTO = p.ID_PROGETTO 
	left outer join VCONTRATTI_FEM_PAGAMENTI cfp on cfp.ID_CONTRATTO_FEM = cf.ID_CONTRATTO_FEM
	inner JOIN GIUSTIFICATIVI GIU ON CF.ID_GIUSTIFICATIVO = GIU.ID_GIUSTIFICATIVO


where 
	p.ORDINE_FASE &gt; 3  
	and ORDINE_STATO = 1 

	and cf.ID_GIUSTIFICATIVO  is not null
                   and cf.CONFIDI= 0
	
order by 
	INTERVENTO,
                   imp.RAGIONE_SOCIALE</CommandText>
      </Query>
      <Fields>
        <Field Name="INTERVENTO">
          <DataField>INTERVENTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID_INTERVENTO">
          <DataField>ID_INTERVENTO</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RAGIONE_SOCIALE">
