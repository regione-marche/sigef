  <DataSets>
    <DataSet Name="dsDecreti">
      <Query>
        <DataSourceName>SIGEF</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdDomandaPagamento">
            <Value>=Parameters!IdDomandaPagamento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        ATTI.NUMERO, ATTI.DATA, ATTI.REGISTRO
FROM            DECRETI_X_DOM_PAG_ESPORTAZIONE INNER JOIN
                         ATTI ON DECRETI_X_DOM_PAG_ESPORTAZIONE.ID_DECRETO = ATTI.ID_ATTO 
WHERE ID_DOMANDA_PAGAMENTO = @IdDomandaPagamento</CommandText>
      </Query>
      <Fields>
        <Field Name="NUMERO">
          <DataField>NUMERO</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DATA">
          <DataField>DATA</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="REGISTRO">
          <DataField>REGISTRO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsRichiestaMandato">
      <Query>
        <DataSourceName>SIGEF</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdDomandaPagamento">
            <Value>=Parameters!IdDomandaPagamento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        RICHIESTA_MANDATO_SEGNATURA, RICHIESTA_MANDATO_DATA
