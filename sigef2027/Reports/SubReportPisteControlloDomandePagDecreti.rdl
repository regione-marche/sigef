    <DataSet Name="dsDomandaPagamento">
      <Query>
        <DataSourceName>SIGEF</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdDomandaPagamento">
            <Value>=Parameters!IdDomandaPagamento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        SEGNATURA_APPROVAZIONE, SEGNATURA, COD_TIPO, ID_DOMANDA_PAGAMENTO, DATA_MODIFICA, DATA_APPROVAZIONE,
(select top 1 g.valore from BANDO_CONFIG g where g.COD_TIPO = 'PREGPDOM' and g.ATTIVO = 1 and g.ID_BANDO = PROGETTO.ID_BANDO) AS pregpdom
FROM            DOMANDA_DI_PAGAMENTO
inner join PROGETTO on progetto.ID_PROGETTO = DOMANDA_DI_PAGAMENTO.ID_PROGETTO
WHERE        (ID_DOMANDA_PAGAMENTO = @IdDomandaPagamento) AND (SEGNATURA IS NOT NULL)</CommandText>
      </Query>
      <Fields>
        <Field Name="SEGNATURA_APPROVAZIONE">
          <DataField>SEGNATURA_APPROVAZIONE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SEGNATURA">
          <DataField>SEGNATURA</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="COD_TIPO">
          <DataField>COD_TIPO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID_DOMANDA_PAGAMENTO">
          <DataField>ID_DOMANDA_PAGAMENTO</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DATA_MODIFICA">
          <DataField>DATA_MODIFICA</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DATA_APPROVAZIONE">
          <DataField>DATA_APPROVAZIONE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="pregpdom">
          <DataField>pregpdom</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCertSp_Testa">
      <Query>
        <DataSourceName>SIGEF</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdDomandaPagamento">
            <Value>=Parameters!IdDomandaPagamento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select top 1 CertSp_Testa.* 
from CertSp_Testa 
	join CertSp_Dettaglio  on CertSp_Dettaglio.IdCertSp = CertSp_Testa.IdCertSp
where CertSp_Dettaglio.Id_Domanda_Pagamento = @IdDomandaPagamento
	and CertSp_Dettaglio.Selezionata = 1
	and CertSp_Testa.Definitivo = 1</CommandText>
      </Query>
