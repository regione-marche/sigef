    <DataSet Name="AnagraficaAzienda">
      <Query>
        <DataSourceName>SIGEF</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdImpresa">
            <Value>=Parameters!IdImpresa.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
V.CUAA,
V.CODICE_FISCALE AS PIVA_CF,
V.RAGIONE_SOCIALE,
V.REGISTRO_IMPRESE_NUMERO,
V.DIMENSIONE_IMPRESA,
V.CODICE_INPS,
V.DESCRIZIONE,
V.REA_NUMERO,
V.REA_ANNO,
V.COD_FORMA_GIURIDICA,
IND.VIA,
IND.LOCALITA,
IND.TELEFONO,
IND.FAX,
IND.EMAIL,
IND.COMUNE AS COMUNE, 
IND.CAP,
IND.ISTAT AS ISTAT_COMUNE,
IND.COD_PROVINCIA AS ISTAT_PROVINCIA,
IND.SIGLA AS PROVINCIA,
IND.PEC        
FROM VIMPRESA V
INNER JOIN VINDIRIZZARIO IND ON V.ID_SEDELEGALE_ULTIMO = IND.ID_INDIRIZZARIO
WHERE V.ID_IMPRESA = @IdImpresa</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
