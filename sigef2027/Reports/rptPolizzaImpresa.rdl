    <DataSet Name="ComunicazioneFinanziabilita">
      <Query>
        <DataSourceName>SIGEF</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdDomandaPagamento">
            <Value>=Parameters!IdDomandaPagamento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  A.DATA AS DATA_GRADUATORIA, A.REGISTRO AS REGISTRO_GRADUATORIA, A.NUMERO AS NUM_GRADUATORIA
FROM            PROGETTO_STORICO AS B INNER JOIN
					 ATTI AS A ON B.ID_ATTO = A.ID_ATTO
WHERE     B.ID = ( SELECT MAX(BSUB.ID) FROM PROGETTO_STORICO AS BSUB 
				   WHERE  (BSUB.ID_PROGETTO = ( SELECT        ID_PROGETTO
												FROM            DOMANDA_DI_PAGAMENTO
												WHERE        (ID_DOMANDA_PAGAMENTO = @IdDomandaPagamento))) AND (BSUB.COD_STATO = 'F'))

UNION
SELECT        A.DATA AS DATA_GRADUATORIA, A.REGISTRO AS REGISTRO_GRADUATORIA, A.NUMERO AS NUM_GRADUATORIA
FROM            BANDO_COMUNICAZIONI AS B INNER JOIN
                         ATTI AS A ON B.ID_ATTO = A.ID_ATTO
WHERE        (B.ID_BANDO =
                             (SELECT        ID_BANDO
                               FROM            PROGETTO
                               WHERE        (ID_PROGETTO =
                                                             (SELECT        ID_PROGETTO
                                                               FROM            DOMANDA_DI_PAGAMENTO AS DOMANDA_DI_PAGAMENTO_1
                                                               WHERE        (ID_DOMANDA_PAGAMENTO = @IdDomandaPagamento))))) AND (B.COD_TIPO = 'CFI')</CommandText>
      </Query>
      <Fields>
        <Field Name="DATA_GRADUATORIA">
          <DataField>DATA_GRADUATORIA</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="NUM_GRADUATORIA">
          <DataField>NUM_GRADUATORIA</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="REGISTRO_GRADUATORIA">
          <DataField>REGISTRO_GRADUATORIA</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
