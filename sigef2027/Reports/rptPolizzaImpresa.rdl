    <DataSet Name="dsVariante">
      <Query>
        <DataSourceName>SIGEF</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdDomandaPagamento">
            <Value>=Parameters!IdDomandaPagamento.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select v.ID_VARIANTE, SUM(p.CONTRIBUTO_RICHIESTO) as CONTRIBUTO_VARIANTE from PIANO_INVESTIMENTI p
inner join VARIANTI v on v.ID_VARIANTE = p.ID_VARIANTE
inner join DOMANDA_DI_PAGAMENTO dp on dp.ID_PROGETTO = v.ID_PROGETTO 
where dp.ID_DOMANDA_PAGAMENTO = @IdDomandaPagamento AND ISNULL(p.COD_VARIAZIONE,'X')!='A'
and v.APPROVATA = 1 and v.DATA_APPROVAZIONE &lt; GETDATE()
group by v.ID_VARIANTE</CommandText>
      </Query>
      <Fields>
        <Field Name="ID_VARIANTE">
          <DataField>ID_VARIANTE</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Contributo_Variante">
          <DataField>CONTRIBUTO_VARIANTE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
