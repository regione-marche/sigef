  <DataSources>
    <DataSource Name="SIGEF_SVIL">
      <DataSourceReference>SIGEF_SVIL</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4db058f5-2322-4b29-be0b-ea506e4ac1d5</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SIGEF_SVIL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Asse">
            <Value>=Parameters!Asse.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH ASSI AS
(
SELECT 
TIPO + ' ' + CODICE AS ASSE, ID
FROM vzPROGRAMMAZIONE 
WHERE ID_PADRE = 0 AND COD_TIPO = 'POR20142020L1'
)
SELECT 
'PI ' + DI.CODICE AS PRIORITA_INVESTIMENTO,
ASSI.ASSE,
OM.DESCRIZIONE + ' - ' + 'RA ' + DI2.CODICE AS OBIETTIVO_SPECIFICO,
-- OM.CODICE,
P.TIPO + ' ' + P.CODICE AS AZIONE,
P2.TIPO + ' ' + P2.CODICE AS TIPOLOGIA_INTERVENTO,
P2.DESCRIZIONE AS DESCRIZIONE_INTERVENTO,
-- PSR.CODICE AS FONDO,
-- vB.ID_BANDO,
vB.DESCRIZIONE AS BANDO,
-- vB.COD_ENTE,
vB.ENTE AS REFERENTE,
vB.IMPORTO AS DOTAZIONE,
A.NUMERO AS NUMERO_ATTO,
A.REGISTRO AS REGISTRO_ATTO,
ISNULL(X.TOTALE_CONCESSO_BENEFICIARI,0) AS TOTALE_CONCESSO_BENEFICIARI,
ISNULL(Y.NUMERO_PROGETTI,0) AS PROGETTI_FINANZIATI,
vB.IMPORTO - ISNULL(X.TOTALE_CONCESSO_BENEFICIARI,0) AS ANCORA_DA_UTILIZZARE,
-- Z.IMPORTO_AMMISSIBILE,
-- Z.IMPORTO_AMMESSO,
-- Z.IMPORTO_SANZIONE,
-- Z.IMPORTO_RECUPERO_ANTICIPO,
-- Z.IMPORTO_LIQUIDATO,
Z.IMPORTO_QUIETANZA_LIQUIDATO,
ISNULL(W.CERTIFICABILE_UE,0) AS CERTIFICABILE_UE

FROM
ASSI INNER JOIN vzPROGRAMMAZIONE P ON P.ID_PADRE = ASSI.ID
INNER JOIN vzPROGRAMMAZIONE P2 ON P.ID = P2.ID_PADRE
INNER JOIN vzDIMENSIONI_X_PROGRAMMAZIONE D ON P.ID = D.ID_PROGRAMMAZIONE
INNER JOIN zDIMENSIONI DI ON D.ID_DIMENSIONE = DI.ID AND DI.COD_DIM IN ('PI')
INNER JOIN vzDIMENSIONI_X_PROGRAMMAZIONE D2 ON P.ID = D2.ID_PROGRAMMAZIONE
INNER JOIN zDIMENSIONI DI2 ON D2.ID_DIMENSIONE = DI2.ID AND DI2.COD_DIM IN ('RA')
--LEFT OUTER JOIN vzDIMENSIONI_X_PROGRAMMAZIONE D2 ON P2.ID = D2.ID_PROGRAMMAZIONE
--LEFT OUTER JOIN zDIMENSIONI DI2 ON D2.ID_DIMENSIONE = DI2.ID AND DI2.COD_DIM = 'RA'
INNER JOIN zOB_MISURA OM on P.ID = OM.ID_PROGRAMMAZIONE
INNER JOIN zPSR_ALBERO PA ON P.COD_TIPO = PA.CODICE
INNER JOIN zPSR PSR ON PA.COD_PSR = PSR.CODICE AND PSR.CODICE = 'POR20142020'
INNER JOIN BANDO_PROGRAMMAZIONE B ON P2.ID=B.ID_PROGRAMMAZIONE
LEFT OUTER JOIN vBANDO vB ON B.ID_BANDO = vB.ID_BANDO AND vB.COD_STATO IN ('C','G','L')
INNER JOIN ATTI A ON vB.ID_ATTO = A.ID_ATTO
LEFT OUTER JOIN (
	SELECT X.ID_BANDO, SUM(TOTALE_CONTRIBUTO_ISTRUITO) AS TOTALE_CONCESSO_BENEFICIARI FROM 
	(
		SELECT P.ID_BANDO, P.ID_PROGETTO, ISNULL(SUM(CASE PI.COD_TIPO_INVESTIMENTO WHEN 2 /*PER IL MUTUO SOMMO SOLO L'AMMONTARE*/ THEN ISNULL(COSTO_INVESTIMENTO,0) 
		ELSE ISNULL(COSTO_INVESTIMENTO,0)+ISNULL(SPESE_GENERALI,0) END),0) AS TOTALE_SPESA_ISTRUITA,
		(CASE WHEN ISNULL(B.IMPORTO_MAX,0) = 0 THEN SUM(ISNULL(PI.CONTRIBUTO_RICHIESTO,0)) ELSE (CASE WHEN SUM(ISNULL(PI.CONTRIBUTO_RICHIESTO,0)) &gt; ISNULL(B.IMPORTO_MAX,0) THEN ISNULL(B.IMPORTO_MAX,0) ELSE SUM(ISNULL(PI.CONTRIBUTO_RICHIESTO,0)) END) END ) AS TOTALE_CONTRIBUTO_ISTRUITO
	FROM PIANO_INVESTIMENTI PI INNER JOIN vPROGETTO P ON PI.ID_PROGETTO=P.ID_PROGETTO
	LEFT JOIN BANDO_TIPO_INVESTIMENTI B ON P.ID_BANDO=B.ID_BANDO AND B.COD_TIPO_INVESTIMENTO=7
	WHERE PI.ID_VARIANTE IS NULL AND PI.ID_INVESTIMENTO_ORIGINALE IS NOT NULL AND P.COD_STATO IN ('F','V','S','O','T','X','C','Y','E','R')  --INSERIRE STATI VALIDI
	GROUP BY P.ID_PROGETTO, B.IMPORTO_MAX, P.ID_BANDO
	) AS X
	GROUP BY X.ID_BANDO
) AS X ON X.ID_BANDO = B.ID_BANDO
LEFT OUTER JOIN
(
	SELECT P.ID_BANDO, COUNT(P.ID_PROGETTO) AS NUMERO_PROGETTI
	FROM vPROGETTO P 
	WHERE P.COD_STATO IN ('F','V','S','O','T','X','C','Y','E','R')  --INSERIRE STATI VALIDI
	GROUP BY P.ID_BANDO
) AS Y ON Y.ID_BANDO = B.ID_BANDO
LEFT OUTER JOIN (
	SELECT P.ID_BANDO, SUM (ISNULL(E.IMPORTO_AMMISSIBILE,0)) AS IMPORTO_AMMISSIBILE, SUM (ISNULL(E.IMPORTO_AMMESSO,0)) AS IMPORTO_AMMESSO, SUM(ISNULL(E.IMPORTO_SANZIONE,0)) AS IMPORTO_SANZIONE, SUM(ISNULL(E.IMPORTO_RECUPERO_ANTICIPO, 0)) AS IMPORTO_RECUPERO_ANTICIPO, SUM(ISNULL(E.IMPORTO_LIQUIDATO, 0)) AS IMPORTO_LIQUIDATO, SUM(ISNULL(L.QUIETANZA_IMPORTO, 0)) AS IMPORTO_QUIETANZA_LIQUIDATO
	FROM PROGETTO P 
	LEFT OUTER JOIN DOMANDA_DI_PAGAMENTO_ESPORTAZIONE E ON P.ID_PROGETTO = E.ID_PROGETTO
	LEFT OUTER JOIN DOMANDA_DI_PAGAMENTO D ON E.ID_DOMANDA_PAGAMENTO = D.ID_DOMANDA_PAGAMENTO
	LEFT OUTER JOIN DOMANDA_PAGAMENTO_LIQUIDAZIONE L ON L.ID_PROGETTO = P.ID_PROGETTO AND LIQUIDATO = 1
	GROUP BY P.ID_BANDO
) AS Z ON Z.ID_BANDO = B.ID_BANDO
LEFT OUTER JOIN (
	SELECT ID_BANDO, SUM(ISNULL(QUOTAUE,0)) AS CERTIFICABILE_UE
	FROM vCertSp_Dettaglio 
	WHERE SELEZIONATA = 1 AND DEFINITIVO = 1
	GROUP BY ID_BANDO
) AS W ON W.ID_BANDO = B.ID_BANDO
WHERE P2.DESCRIZIONE NOT LIKE '%DA ELIMINARE%' and ASSE like @Asse --AND vB.ID_BANDO IS NOT NULL 
ORDER BY ASSI.ASSE, P.TIPO + ' ' + P.CODICE

