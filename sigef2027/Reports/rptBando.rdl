    <DataSet Name="dsDatiBando">
      <Query>
        <DataSourceName>SIGEF_TEST</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CodStato">
            <Value>=Parameters!CodStato.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IdProgrammazione">
            <Value>=Parameters!IdProgrammazione.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT RB.DESCRIZIONE, RB.DATA_SCADENZA, NUM_DOMANDE, RB.COD_STATO, TOTALE_INVESTIMENTI, TOTALE_CONTRIBUTO, TOT_INTERESSI,
