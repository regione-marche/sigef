    <DataSet Name="TipoDomanda">
      <Query>
        <DataSourceName>SIGEF_SVIL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdComunicazione">
            <Value>=Parameters!IdComunicazione.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
select CASE WHEN P.ID_DOMANDA_PAGAMENTO IS NULL 
AND P.ID_VARIANTE IS NULL THEN P.ID_PROGETTO END AS NUM_DOMANDA_AIUTO, 
CASE WHEN P.ID_DOMANDA_PAGAMENTO IS NOT NULL THEN P.ID_DOMANDA_PAGAMENTO END
AS NUM_DOMANDA_PAGAMENTO, 
CASE WHEN P.ID_VARIANTE IS NOT NULL THEN P.ID_VARIANTE END
AS NUM_VARIANTE, ISNULL(D.DESCRIZIONE, '')  +  
ISNULL(V.TIPO_VARIANTE, '') AS TIPO
from PROGETTO_COMUNICAZIONI p
left join VDOMANDA_DI_PAGAMENTO d ON P.ID_DOMANDA_PAGAMENTO = D.ID_DOMANDA_PAGAMENTO
LEFT JOIN VVARIANTI V ON P.ID_VARIANTE = V.ID_VARIANTE
WHERE P.ID = @IdComunicazione</CommandText>
      </Query>
      <Fields>
        <Field Name="NUM_DOMANDA_AIUTO">
          <DataField>NUM_DOMANDA_AIUTO</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NUM_DOMANDA_PAGAMENTO">
          <DataField>NUM_DOMANDA_PAGAMENTO</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NUM_VARIANTE">
          <DataField>NUM_VARIANTE</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TIPO">
          <DataField>TIPO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
