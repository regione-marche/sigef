    <DataSet Name="dsSDA">
      <Query>
        <DataSourceName>SIGEF_SVIL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdComunicazione">
            <Value>=Parameters!IdComunicazione.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @IdProgetto int 
select @IdProgetto = ID_PROGETTO FROM PROGETTO_COMUNICAZIONI WHERE ID = @IdComunicazione

declare @ENTE VARchar(10) 
select @ENTE = COD_ENTE from BANDO B 
inner join PROGETTO p on B.ID_BANDO = P.ID_BANDO
where ID_PROGETTO = @IdProgetto 

select * from UFFICI_DECENTRATI
where CODICE = @ENTE and attivo = 1</CommandText>
      </Query>
