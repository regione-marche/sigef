  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SIGEF</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IdErogazione">
            <Value>=Parameters!IdErogazione.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
convert(varchar(max), a.NUMERO)+'|'+ CONVERT(varchar(10),a.DATA,103) +'|'+a.REGISTRO as DECRETO,
efd.IMPORTO,
efd.IMPORTO_AMMESSO
 from EROGAZIONE_FEM_X_DECRETI efd
inner join ATTI a on a.ID_ATTO = efd.ID_DECRETO
where ID_EROGAZIONE = @IdErogazione</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
