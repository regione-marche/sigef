  <DataSets>
    <DataSet Name="DsRupValidatori">
      <Query>
        <DataSourceName>SIGEF_TEST</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AIUTI">
            <Value>=Parameters!AIUTI.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>If @AIUTI = 1
begin

select distinct 'RUP' as RUOLO,r.NOMINATIVO,ISNULL(u.EMAIL,'') as INDIRIZZO_EMAIL, b.ENTE  from vBANDO_RESPONSABILI r 
inner join vUTENTI u on u.ID_UTENTE = r.ID_UTENTE
inner join vbando b on b.ID_BANDO = r.ID_BANDO
inner join BANDO_CONFIG g on g.ID_BANDO= b.ID_BANDO and g.ATTIVO = 1 and g.COD_TIPO ='NATURACUP' 
and  (g.VALORE like '%06%' or g.VALORE  like '%07%')
inner join zPROGRAMMAZIONE z on z.ID = b.ID_PROGRAMMAZIONE and z.COD_TIPO like '%POR20142020L%'

where r.ATTIVO = 1 and r.PROVINCIA is null

union

select distinct 'VALIDATORE' as RUOLO,u.NOMINATIVO,ISNULL(u.EMAIL,'') as INDIRIZZO_EMAIL,u.ENTE from bando_validatori v 
inner join vbando b on b.ID_BANDO = v.ID_BANDO
inner join BANDO_CONFIG g on g.ID_BANDO= b.ID_BANDO and g.ATTIVO = 1 and g.COD_TIPO ='NATURACUP' and ( g.VALORE  like '%06%' or g.VALORE  like '%07%')
inner join zPROGRAMMAZIONE z on z.ID = b.ID_PROGRAMMAZIONE and z.COD_TIPO like '%POR20142020L%'
inner join vUTENTI u on u.ID_UTENTE = v.ID_UTENTE
where v.ATTIVO = 1

order by 1,4
END
else
begin

select distinct 'RUP' as RUOLO,r.NOMINATIVO,ISNULL(u.EMAIL,'') as INDIRIZZO_EMAIL, b.ENTE  from vBANDO_RESPONSABILI r 
inner join vUTENTI u on u.ID_UTENTE = r.ID_UTENTE
inner join vbando b on b.ID_BANDO = r.ID_BANDO
inner join BANDO_CONFIG g on g.ID_BANDO= b.ID_BANDO and g.ATTIVO = 1 and g.COD_TIPO ='NATURACUP' 
and  g.VALORE not like '%06%' and g.VALORE not like '%07%'
inner join zPROGRAMMAZIONE z on z.ID = b.ID_PROGRAMMAZIONE and z.COD_TIPO like '%POR20142020L%'

where r.ATTIVO = 1 and r.PROVINCIA is null

union

select distinct 'VALIDATORE' as RUOLO,u.NOMINATIVO,ISNULL(u.EMAIL,'') as INDIRIZZO_EMAIL,u.ENTE from bando_validatori v 
inner join vbando b on b.ID_BANDO = v.ID_BANDO
inner join BANDO_CONFIG g on g.ID_BANDO= b.ID_BANDO and g.ATTIVO = 1 and g.COD_TIPO ='NATURACUP' and  g.VALORE not like '%06%' and g.VALORE not like '%07%'
inner join zPROGRAMMAZIONE z on z.ID = b.ID_PROGRAMMAZIONE and z.COD_TIPO like '%POR20142020L%'
inner join vUTENTI u on u.ID_UTENTE = v.ID_UTENTE
where v.ATTIVO = 1

order by 1,4
END</CommandText>
      </Query>
